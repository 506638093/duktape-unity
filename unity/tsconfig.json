{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "sourceRoot": "Assets/Examples/Scripts/Source",
        "outDir": "Assets/Examples/Scripts/Generated",
        "typeRoots": [
            "Assets/Duktape/Generated",
            "Assets/Duktape/Typings",
            "Assets/Examples/Source/Generated",
            "Assets/Examples/Scripts/Typings"
        ],
        "moduleResolution": "node",
        "types": [
            "node"
        ],
        // "listEmittedFiles": true,
        "noImplicitAny": false,
        "allowJs": false,
        "inlineSourceMap": false,
        "sourceMap": true
    },
    "exclude": [
        "Assets/Generated/**/*"
    ]
}